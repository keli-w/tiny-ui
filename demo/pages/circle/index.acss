.circle-container {
  display: flex;
  flex-direction: row;
  justify-content: space-evenly;
  margin-top: 30rpx;
}

.button_list {
  display: flex;
  flex-direction: row;
  justify-content: space-evenly;
  width: 700rpx;
  margin-top: 60rpx;
  margin-bottom: 30rpx;
}

.button_list button {
  min-width: 10rpx;
  height: 60rpx;
  padding: 0 10rpx;
  font-size: 20rpx;
  line-height: 60rpx;
  text-align: center;
  border-radius: 10rpx;
}
